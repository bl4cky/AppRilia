<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.blackrio.apprilia" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Activity.Profil"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity.Login"
            android:label="@string/title_activity_register" >
        </activity>
        <activity
            android:name=".Activity.Register"
            android:label="@string/title_activity_profil" >
        </activity>
        <activity
            android:name=".Activity.ValueAnalyse"
            android:label="@string/title_activity_value_analyse" >
        </activity>
        <activity
            android:name=".Activity.ToDo"
            android:label="@string/title_activity_to_do" >
        </activity>
        <activity
            android:name=".Activity.ServiceList"
            android:label="@string/title_activity_sercive_list" >
        </activity>
    </application>

</manifest>
